	if (loopAnimation<=6){

		cyclePowerUpAnimation=cyclePowerUpAnimation+1;

		
		loopAnimation=loopAnimation+1;

	}

	if ((loopAnimation>6)&&(loopAnimation<=14)){

			cyclePowerUpAnimation=cyclePowerUpAnimation-1;
